            int s = b;
            a = c;
            b = z;
            c = s;
            Console.WriteLine("A = " + a);
            Console.WriteLine("B = " + b);
            Console.WriteLine("C = " + c);
            */

            /* Задание 4
            Console.Write("Введите значение х: ");
            int x = Convert.ToInt32(Console.ReadLine());
            int y = (int)(3 * Math.Pow(x, 6) - 6 * Math.Pow(x, 2) - 7);

            int y = (int)(4 * Math.Pow(x - 3, 6) - 7 * Math.Pow(x - 3, 3) + 2);
            Console.WriteLine("y = 4(x−3)^6 − 7(x−3)^3 + 2 = " + y);
            */

            /* Задание 6

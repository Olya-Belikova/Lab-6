            int a3 = a2 * a; // A^3
            int a5 = a3 * a2; // A^5
            int a10 = a5 * a5; // A^10
            int a15 = a10 * a5; // A^15
            Console.WriteLine("A^2 =  " + a2);
            Console.WriteLine("A^3 =  " + a3);
            Console.WriteLine("A^5 =  " + a5);
            Console.WriteLine("A^10 =  " + a10);
